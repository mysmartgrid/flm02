config interface loopback
        option ifname   lo
        option proto    static
        option ipaddr   127.0.0.1
        option netmask  255.0.0.0

config interface wan
        option ifname   ath0
        option proto    dhcp
	option accept_ra 1

config interface lan
        option ifname   eth0
        option proto    static
        option ipaddr   192.168.255.1
        option netmask  255.255.255.0
